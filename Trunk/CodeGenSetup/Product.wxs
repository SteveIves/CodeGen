    <!--
     These properties are used to capture the user input from the two edit controls in the
     custom user registration UI page. The Id's are also special windows installer varibale
     names, that will br pre-populated with the user and company names from the Windows
     registration registry entries, thus providing default values for the installer UI.
    -->
    <Property Id="USERNAME"    Secure="yes" />
    <Property Id="COMPANYNAME" Secure="yes" />

    <!-- ************************************************************************************************************* -->
    <!-- Add a checkbox to the final UI dialog and configure it to open the documentation -->

    <!--<Property Id="WIXUI_EXITDIALOGOPTIONALCHECKBOXTEXT" Value="Open the CodeGen documentation now" />
    <Property Id="WixShellExecTarget" Value="[#CodeGen.chm]" />
    <CustomAction Id="LaunchDocumentation" BinaryKey="WixCA" DllEntry="WixShellExec" Impersonate="yes" />-->

