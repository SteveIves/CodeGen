    <!-- Setting these properties causes these URL's to be displayed when the application is selected in "Programs and Features"-->

    <Property Id="ARPHELPLINK"      Value="http://codegen.codeplex.com/documentation" />
    <Property Id="ARPURLINFOABOUT"  Value="http://codegen.codeplex.com/discussions" />
    <Property Id="ARPURLUPDATEINFO" Value="http://codegen.codeplex.com/downloads" />

    <MajorUpgrade DowngradeErrorMessage="A newer version of [ProductName] is already installed." />

    <MediaTemplate EmbedCab="yes" />

