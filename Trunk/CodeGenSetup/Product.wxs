    Name="CodeGen"
    Version="5.0.4"
    Id="{2B8495AE-D8E0-4AF8-9D60-4B7BB30064D1}"
    Language="1033"
    Manufacturer="Synergex International Corporation"
    UpgradeCode="{0B992863-4C0E-4769-ABBC-49DB5C3A09A6}">

